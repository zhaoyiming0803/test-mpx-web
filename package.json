{
  "name": "test-mpx-web",
  "version": "1.0.0",
  "description": "A mpx project",
  "main": "index.js",
  "scripts": {
    "watch": "node ./build/build.js -w",
    "watch:prod": "node ./build/build.js -w -p",
    "build": "node ./build/build.js -p",
    "build:dev": "node ./build/build.js",
    "watch:cross": "npx npm-run-all --parallel 'watch --wx --ali --swan --web' httpserver",
    "watch:prod:cross": "npx npm-run-all --parallel 'watch:prod --wx --ali --swan --web' httpserver",
    "build:cross": "npm run build --wx --ali --swan --web",
    "build:dev:cross": "npm run build:dev --wx --ali --swan --web",
    "watch:web": "npx npm-run-all --parallel watchwebpure httpserver",
    "watchwebpure": "npm run watch --web",
    "httpserver": "npx http-server dist/web",
    "help": "node ./build/build.js --help"
  },
  "author": "zhaoyiming",
  "license": "ISC",
  "dependencies": {
    "vue": "^2.6.10",
    "vue-i18n": "^8.15.3",
    "@mpxjs/api-proxy": "latest",
    "@mpxjs/core": "latest",
    "@mpxjs/fetch": "latest"
  },
  "devDependencies": {
    "@mpxjs/mock": "^2.6.44",
    "@mpxjs/webpack-plugin": "latest",
    "acorn": "^7.3.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "chalk": "^2.3.2",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^0.28.11",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "latest",
    "http-server": "^0.12.0",
    "jquery": "^3.5.1",
    "npm-run-all": "^4.1.5",
    "ora": "^2.0.0",
    "path": "^0.12.7",
    "rimraf": "^2.6.2",
    "safe-require": "^1.0.3",
    "style-loader": "^1.0.1",
    "stylus": "^0.54.5",
